{
  "manifest_version": 3,
 
  "key": "",
 
  "name": "gpsio",
  "description": "GPS IO",
  "version": "1.0",
  
  "action": {
    "default_icon": "gps.png",
    "default_popup": "popup.html"
   },

  "permissions": [
    "nativeMessaging","storage"
    ],
  
  "background": {
    "service_worker": "background.js"
  },
    
  "content_scripts": [{"matches": ["https://*/*","http://*/*"], "js": ["content_script.js"]}]
}
